  Action = ExtraSetup
    setupRequired(root v6_12_06a -q +c2:+prof)
    setupRequired(boost v1_66_0a -q +c2:+prof)
    setupRequired(jsoncpp v1_7_7c -q +c2:+prof)
    setupRequired(jsonnet v0_11_2 -q +c2:+prof)
